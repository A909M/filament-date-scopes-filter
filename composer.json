{
    "name": "aymanalhattami/filament-date-scopes-filter",
    "description": "Filament Date Scopes Filter",
    "keywords": [
        "aymanalhattami",
        "filament-date-scopes-filter",
        "filament",
        "date scopes filter",
        "date filter"
    ],
    "homepage": "https://github.com/aymanalhattami/filament-date-scopes-filter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ayman Alhattami",
            "email": "ayman.m.alhattami@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "filament/tables": "^3.0",
        "laracraft-tech/laravel-date-scopes": "^2.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "AymanAlhattami\\FilamentDateScopesFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AymanAlhattami\\FilamentDateScopesFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AymanAlhattami\\FilamentDateScopesFilter\\FilamentDateScopesFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
